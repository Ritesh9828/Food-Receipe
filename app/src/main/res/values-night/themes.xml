<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.HungryHead" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/CeladonBlue</item>
        <item name="colorPrimaryVariant">@color/CeladonBlue</item>
        <item name="colorOnPrimary">@color/Almond</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/Almond</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/DutchWhite</item>
        <item name="bottomSheetDialogTheme">@style/AppBottomSheetDialogTheme</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>

        <!-- Customize your theme here. -->
    </style>
</resources>